{
  "name": "interactive-disco-website",
  "version": "1.0.0",
  "description": "Portfolio site.",
  "author": "Alex Desharnais",
  "license": "ISC",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prebuild": "rimraf build",
    "build": "webpack --config config/webpack.config.prod.js --mode production --progress",
    "dev": "webpack-dev-server --config config/webpack.config.dev.js --open --port 3000 --compress --mode development",
    "prod": "node src/server.js",
    "test": "jest --no-watch --onlyChanged",
    "lint": "eslint 'src/*.js'"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.6.0",
    "lint-staged": "^8.1.5",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.6.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "express": "^4.17.1",
    "gsap": "^2.1.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-lazyload": "^2.6.1",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^2.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "https://interactivedisco.com",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
